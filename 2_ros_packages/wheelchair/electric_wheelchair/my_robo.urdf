<?xml version="1.0"?>
<robot name="wheelchair" >
 <!--<link name="world"/> -->

  <link name="base_link">

    <visual>
      <origin rpy="0 0 3.1415" xyz="0.22 0 0.01"/>
      <!-- <origin rpy="0 0 3.1415" xyz="0.296 0.00 0.150"/> -->
      <!-- <origin rpy="0 0 0" xyz="0.296 0.00 0.150"/> -->
      <geometry>
        <!-- <mesh filename="package://electric_wheelchair/mesh/base.dae"/> -->
        <!-- <mesh filename="/home/yugo/Desktop/daefile/base.dae"/> -->
        <box size="0.592 0.492 0.300"/>
      </geometry>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.296 0.00 0.150"/>
      <geometry>
        <box size="0.592 0.492 0.300"/>
      </geometry>
    </collision>

   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="0.500"/>
    <inertia ixx="0.0020833333" ixy="0" ixz="0" iyy="0.0070833333" iyz="0" izz="0.0083333333"/>
   </inertial>
  </link>

  <!-- <joint name="world_joint" type="fixed">　
    <parent link="world"/> 
    <child link="base_link"/>　
  </joint> -->

  <link name="left_wheel_link">
    <visual>
      <geometry>
        <!-- <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/> -->
        <cylinder radius="0.1325" length="0.055"/>
      </geometry>
      <origin xyz="-0.22 0 -0.25" rpy="1.5709 0 0"/>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.1325" length="0.05"/>
      </geometry>
    </collision>

   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.000"/><!--左後輪質量-->
    <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/>
   </inertial>

  </link>

  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <origin rpy="-1.570796 0 0" xyz="0 0.246 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="right_wheel_link">
    <visual>
      <geometry>
        <!-- <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/> -->
        <cylinder radius="0.1325" length="0.055"/>
      </geometry>
      <origin xyz="-0.23 0 -0.25" rpy="1.5709 0 0"/>
      <material name="black"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.1325" length="0.055"/>
      </geometry>
    </collision>

   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.00"/><!--右後輪質量-->
    <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/>
   </inertial>

  </link>

  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel_link"/>
    <origin rpy="1.5707 0 0" xyz="0 -0.246 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="left_caster_link">
    <visual>
      <geometry>
        <!-- <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/> -->
        <cylinder radius="0.127" length="0.055"/>
      </geometry>
      <origin xyz="0.36 0 0.24" rpy="1.5707 0 0"/>
      <!-- <origin xyz="0 0 0" rpy="1.5707 0 0"/> -->
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.127" length="0.055"/>
      </geometry>
    </collision>

   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.000"/><!--左前輪質量-->
    <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025"  iyz="0" izz="0.00025"/>
   </inertial>

  </link>

  <joint name="left_caster_joint" type="continuous">
   <parent link="base_link"/>
   <child link="left_caster_link"/>
   <origin rpy="1.5707 0 0" xyz="0.592 0.246 0"/>
   <axis xyz="0 0 1"/>
  </joint>

 <link name="right_caster_link">
   <visual>
     <geometry>
       <!-- <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/> -->
       <cylinder radius="0.127" length="0.055"/>
     </geometry>
     <origin xyz="0.36 0 0.23" rpy="1.5707 0 0"/>
          <!-- <origin xyz="0 0 0" rpy="1.5707 0 0"/> -->
     <material name="green">
       <color rgba="0 1 0 1"/>
     </material>
   </visual>

   <collision>
     <geometry>
       <cylinder radius="0.127" length="0.055"/>
     </geometry>
   </collision>

  <inertial>
   <origin xyz="0 0 0"/>
   <mass value="1.000"/><!--右前輪質量-->
   <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025"  iyz="0" izz="0.00025"/>
  </inertial>

 </link>

 <joint name="right_caster_joint" type="continuous">
  <parent link="base_link"/>
  <child link="right_caster_link"/>
  <origin rpy="1.5707 0 0" xyz="0.592 -0.246 0"/>
  <axis xyz="0 0 1"/>
  </joint>

 <link name="base_footprint"/>
  <joint name="base_link_joint" type="fixed">
   <parent link="base_footprint"/>
   <child link="base_link"/>
   <origin xyz="0 0 0.100"/>
 </joint>

<!-- ===============  Transmission =============== -->

 <transmission name="left_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="left_wheel_joint">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
  <actuator name="left_wheel_motor">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  <mechanicalReduction>30</mechanicalReduction>
  </actuator>
 </transmission>

 <transmission name="right_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="right_wheel_joint">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
  <actuator name="right_wheel_motor">
  <hardwareInterface>VelocityJointInterface</hardwareInterface>
  <mechanicalReduction>30</mechanicalReduction>
  </actuator>
 </transmission>

<!-- =============== Gazebo =============== -->

  <gazebo>
   <plugin name="gazebo_ros2_control" filename="libgazebo_ros2_control.so">
    <robotNamespace>wheelchair</robotNamespace>
    <!-- <ros2_control namespace="wheelchair" /> -->
   </plugin>
  </gazebo>

 <gazebo reference="base_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.05" />
  <mu2 value="0.05" />
 </gazebo>

 <gazebo reference="left_wheel_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>

 <gazebo reference="right_wheel_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>

 <gazebo reference="left_caster_link">
  <selfCollide>true</selfCollide>
  <!-- This caster is no frictional resistance. -->
  <mu1 value="0.0" />
  <mu2 value="0.0" />
 </gazebo>

 <gazebo reference="right_caster_link">
  <selfCollide>true</selfCollide>
  <!-- This caster is no frictional resistance. -->
  <mu1 value="0.0" />
  <mu2 value="0.0" />
 </gazebo>

</robot>

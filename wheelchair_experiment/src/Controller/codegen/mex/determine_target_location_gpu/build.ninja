# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/MATLAB/R2024b
startdir = /home/student/Document/10号館4階通路走行/10号館4階通路走行_2025_07_02/capsuele_v5.3_複数歩行者/src/Controller

# Toolchain information
cc = /usr/bin/gcc
cxx = /usr/bin/g++
linker = /usr/bin/gcc
linkerxx = /usr/bin/g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -fwrapv -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fno-predictive-commoning -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$matlabdir/toolbox/gpucoder/src/gpucoderrt/export/include/gpucoderrt" -I "$matlabdir/sys/cuda/glnxa64/cuda/include" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMW_CUDA_ARCH=750 -DMW_GPU_MEMORY_MANAGER -DMODEL=determine_target_location_gpu_mex -DMW_NEEDS_VERSION_H -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -fwrapv -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fno-predictive-commoning -fvisibility=hidden -fno-gnu-unique -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/toolbox/gpucoder/src/gpucoderrt/export/include/gpucoderrt" -I "$matlabdir/sys/cuda/glnxa64/cuda/include" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMW_CUDA_ARCH=750 -DMW_GPU_MEMORY_MANAGER -DMODEL=determine_target_location_gpu_mex -DMW_NEEDS_VERSION_H -c 

ldflags =  -Xlinker --no-undefined -Xlinker -rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -Xlinker -Bsymbolic -o determine_target_location_gpu_mex.mexa64 -Xlinker --exclude-libs,ALL -L"$matlabdir/sys/cuda/glnxa64/cuda/lib64" -L"$matlabdir/bin/glnxa64" -lmwgpucoderrt -lemlrt -lcovrt -lut -lmwmathutil -arch sm_75 -I"$matlabdir/sys/cuda/glnxa64/cuda/include" -l:libcudart.so.12 -l:libcublas.so.12 -l:libcusolver.so.11 -l:libcufft.so.11 -l:libcurand.so.10 -l:libcusparse.so.12 -Xnvlink -w  -Wno-deprecated-gpu-targets

ldxxflags =  -Xlinker --no-undefined  -shared -lstdc++ -Xlinker --as-needed -Xlinker -rpath-link,$matlabdir/bin/glnxa64 -L"$matlabdir/bin/glnxa64" -Xlinker -rpath-link,$matlabdir/extern/bin/glnxa64 -L"$matlabdir/extern/bin/glnxa64" -lMatlabDataArray -lmx -lmex -lm -lmat  -Xlinker -Bsymbolic -o determine_target_location_gpu_mex.mexa64 -Xlinker --exclude-libs,ALL -L"$matlabdir/sys/cuda/glnxa64/cuda/lib64" -L"$matlabdir/bin/glnxa64" -lmwgpucoderrt -lemlrt -lcovrt -lut -lmwmathutil -arch sm_75 -I"$matlabdir/sys/cuda/glnxa64/cuda/include" -l:libcudart.so.12 -l:libcublas.so.12 -l:libcusolver.so.11 -l:libcufft.so.11 -l:libcurand.so.10 -l:libcusparse.so.12 -Xnvlink -w  -Wno-deprecated-gpu-targets

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldxxflags

cudac = nvcc
linkercuda = nvcc

cudaflags =  -rdc=true -Wno-deprecated-gpu-targets -Xcompiler -fPIC,-fexceptions,-fno-omit-frame-pointer,-pthread -Xcudafe "--display_error_number --diag_suppress=2381 --diag_suppress=unsigned_compare_with_zero --diag_suppress=useless_type_qualifier_on_return_type" -D_GNU_SOURCE -DMATLAB_MEX_FILE -noprof -ldir "$matlabdir/bin/glnxa64" -Xcompiler -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/toolbox/gpucoder/src/gpucoderrt/export/include/gpucoderrt" -I "$matlabdir/sys/cuda/glnxa64/cuda/include" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMW_CUDA_ARCH=750 -DMW_GPU_MEMORY_MANAGER -DMODEL=determine_target_location_gpu_mex -DMW_NEEDS_VERSION_H -c -arch sm_75 -I "$matlabdir/simulink/include" -I "$matlabdir/toolbox/shared/simtargets"
rule cudac
    command = $cudac $cudaflags $in -o $out

rule linkcuda
    command = $linkercuda $in $linkobjs $ldflags

# Build
build $builddir/MWLaunchParametersUtilities.o : cudac $root/MWLaunchParametersUtilities.cpp
build $builddir/determine_target_location_gpu_data.o : cudac $root/determine_target_location_gpu_data.cu
build $builddir/rt_nonfinite.o : cudac $root/rt_nonfinite.cu
build $builddir/determine_target_location_gpu_initialize.o : cudac $root/determine_target_location_gpu_initialize.cu
build $builddir/determine_target_location_gpu_terminate.o : cudac $root/determine_target_location_gpu_terminate.cu
build $builddir/determine_target_location_gpu.o : cudac $root/determine_target_location_gpu.cu
build $builddir/_coder_determine_target_location_gpu_api.o : cudac $root/interface/_coder_determine_target_location_gpu_api.cu
build $builddir/_coder_determine_target_location_gpu_mex.o : cudac $root/interface/_coder_determine_target_location_gpu_mex.cu
build $builddir/determine_target_location_gpu_emxutil.o : cudac $root/determine_target_location_gpu_emxutil.cu
build $builddir/_coder_determine_target_location_gpu_info.o : cxx $root/interface/_coder_determine_target_location_gpu_info.cpp
build $builddir/cpp_mexapi_version.o : cudac $root/cpp_mexapi_version.cpp

# Link
build $root/determine_target_location_gpu_mex.mexa64 : linkcuda $builddir/MWLaunchParametersUtilities.o $builddir/determine_target_location_gpu_data.o $builddir/rt_nonfinite.o $builddir/determine_target_location_gpu_initialize.o $builddir/determine_target_location_gpu_terminate.o $builddir/determine_target_location_gpu.o $builddir/_coder_determine_target_location_gpu_api.o $builddir/_coder_determine_target_location_gpu_mex.o $builddir/determine_target_location_gpu_emxutil.o $builddir/_coder_determine_target_location_gpu_info.o $builddir/cpp_mexapi_version.o 

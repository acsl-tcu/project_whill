# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/MATLAB/R2024b
startdir = /home/student/Document/10号館4階通路走行/10号館4階通路走行_2025_07_02/capsuele_v5.3_複数歩行者/src/Controller

# Toolchain information
cc = /usr/bin/gcc
cxx = /usr/bin/g++
linker = /usr/bin/gcc
linkerxx = /usr/bin/g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -fwrapv -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fno-predictive-commoning -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=determine_target_location_mex -msse2 -fno-predictive-commoning -Wno-stringop-overflow -Wno-maybe-uninitialized -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -fwrapv -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -DNDEBUG -fno-predictive-commoning -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=determine_target_location_mex -msse2 -fno-predictive-commoning -Wno-stringop-overflow -Wno-maybe-uninitialized -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -Wl,-Bsymbolic -o determine_target_location_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,determine_target_location.map

ldxxflags =  -pthread -Wl,--no-undefined  -shared -lstdc++ -Wl,--as-needed -Wl,-rpath-link,$matlabdir/bin/glnxa64 -L"$matlabdir/bin/glnxa64" -Wl,-rpath-link,$matlabdir/extern/bin/glnxa64 -L"$matlabdir/extern/bin/glnxa64" -lMatlabDataArray -lmx -lmex -lm -lmat  -Wl,-Bsymbolic -o determine_target_location_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,determine_target_location.map

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldxxflags

# Build
build $builddir/determine_target_location_data.o : cc $root/determine_target_location_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/determine_target_location_initialize.o : cc $root/determine_target_location_initialize.c
build $builddir/determine_target_location_terminate.o : cc $root/determine_target_location_terminate.c
build $builddir/determine_target_location.o : cc $root/determine_target_location.c
build $builddir/assertValidSizeArg.o : cc $root/assertValidSizeArg.c
build $builddir/_coder_determine_target_location_api.o : cc $root/interface/_coder_determine_target_location_api.c
build $builddir/_coder_determine_target_location_mex.o : cc $root/interface/_coder_determine_target_location_mex.c
build $builddir/determine_target_location_emxutil.o : cc $root/determine_target_location_emxutil.c
build $builddir/_coder_determine_target_location_info.o : cc $root/interface/_coder_determine_target_location_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/determine_target_location_mex.mexa64 : link $builddir/determine_target_location_data.o $builddir/rt_nonfinite.o $builddir/determine_target_location_initialize.o $builddir/determine_target_location_terminate.o $builddir/determine_target_location.o $builddir/assertValidSizeArg.o $builddir/_coder_determine_target_location_api.o $builddir/_coder_determine_target_location_mex.o $builddir/determine_target_location_emxutil.o $builddir/_coder_determine_target_location_info.o $builddir/c_mexapi_version.o 
